extends layout

block content
  h1= title
  p Welcome to #{title}
   - var results = event;
  div(class="container-fluid")
    each value in results 
      div(class="row")
        div(class="col-sm-2") #{value.event_Name}
        div(class="col-sm-2") #{value.event_id}
        div(class="col-sm-2") #{value.event_type}